{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": "test/.*\\.e2e-spec\\.ts$",
  "transformIgnorePatterns": ["<rootDir>/prisma/generated/test-client/"],
  "transform": {
    "^.+\\.ts$": [
      "ts-jest",
      {
        "tsconfig": "<rootDir>/tsconfig.spec.json"
      }
    ]
  }
}
